<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:columnCount="5"
    android:orientation="horizontal">
    <Button
        android:id="@+id/ButtonHistory"
        android:text="Hst" />
    <Button
        android:id="@+id/S_calc"
        android:text="Sci" />
    <Button
        android:id="@+id/S_Hist"
        android:layout_gravity="fill"
        android:layout_columnSpan="2"
        android:text="SaveHistory" />
    <Button
        android:id="@+id/Clear_history"
        android:text="CL" />
    <EditText
        android:id="@+id/textViewHis0"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_columnSpan="5"
        android:layout_gravity="fill"
        android:background="#D3D3D3"
        android:scrollHorizontally="false"
        android:scrollbars="vertical"
        android:editable="true"
        android:gravity="left"
        android:textColor="#000000"
        android:focusableInTouchMode="false" />
    <EditText
        android:id="@+id/textViewHis1"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_columnSpan="5"
        android:layout_gravity="fill"
        android:background="#D3D3D3"
        android:scrollHorizontally="false"
        android:scrollbars="vertical"
        android:editable="true"
        android:gravity="left"
        android:textColor="#000000"
        android:focusableInTouchMode="false" />
    <EditText
        android:id="@+id/textViewHis2"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_columnSpan="5"
        android:layout_gravity="fill"
        android:background="#D3D3D3"
        android:scrollHorizontally="false"
        android:scrollbars="vertical"
        android:editable="true"
        android:gravity="left"
        android:textColor="#000000"
        android:focusableInTouchMode="false" />
    <EditText
        android:id="@+id/textViewHis3"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_columnSpan="5"
        android:layout_gravity="fill"
        android:background="#D3D3D3"
        android:scrollHorizontally="false"
        android:scrollbars="vertical"
        android:editable="true"
        android:gravity="left"
        android:textColor="#000000"
        android:focusableInTouchMode="false" />
    <EditText
        android:id="@+id/textViewHis4"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_columnSpan="5"
        android:layout_gravity="fill"
        android:background="#D3D3D3"
        android:scrollHorizontally="false"
        android:scrollbars="vertical"
        android:editable="true"
        android:gravity="left"
        android:textColor="#000000"
        android:focusableInTouchMode="false" />
    <EditText
        android:id="@+id/textViewHis5"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_columnSpan="5"
        android:layout_gravity="fill"
        android:background="#D3D3D3"
        android:scrollHorizontally="false"
        android:scrollbars="vertical"
        android:editable="true"
        android:gravity="left"
        android:textColor="#000000"
        android:focusableInTouchMode="false" />
    <EditText
        android:id="@+id/textViewHis6"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_columnSpan="5"
        android:layout_gravity="fill"
        android:background="#D3D3D3"
        android:scrollHorizontally="false"
        android:scrollbars="vertical"
        android:editable="true"
        android:gravity="left"
        android:textColor="#000000"
        android:focusableInTouchMode="false" />
    <ImageView
        android:layout_gravity="center_horizontal"
        android:paddingTop="1px"
        android:paddingBottom="5px"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:src="@android:drawable/divider_horizontal_bright" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_columnSpan="5"
        android:layout_gravity="fill"
        android:background="#FFFFFF"
        android:text="0"
        android:textColor="@android:color/black"
        android:gravity="right" />
    <Button
        android:id="@+id/memory_clear"
        android:text="MC" />
    <Button
        android:id="@+id/memory_recall"
        android:text="MR" />
    <Button
        android:id="@+id/memory_save"
        android:text="MS" />
    <Button
        android:id="@+id/Memory_plus"
        android:text="M+" />
    <Button
        android:id="@+id/Memory_minus"
        android:text="M-" />
    <Button
        android:text="" />
    <Button
        android:id="@+id/Clear_Entry"
        android:text="CE" />
    <Button
        android:id="@+id/Clear"
        android:text="C" />
    <Button
        android:id="@+id/UnaryOperator_Negate"
        android:text="+-" />
    <Button
        android:id="@+id/UnaryOperator_Sqrt"
        android:text="Sqrt" />
    <Button
        android:id="@+id/Number_Click7"
        android:text="7" />
    <Button
        android:id="@+id/Number_Click8"
        android:text="8" />
    <Button
        android:id="@+id/Number_Click9"
        android:text="9" />
    <Button
        android:id="@+id/BinaryOperator_ClickDiv"
        android:text="/" />
    <Button
        android:text="" />
    <Button
        android:id="@+id/Number_Click4"
        android:text="4" />
    <Button
        android:id="@+id/Number_Click5"
        android:text="5" />
    <Button
        android:id="@+id/Number_Click6"
        android:text="6" />
    <Button
        android:id="@+id/BinaryOperator_ClickMul"
        android:text="*" />
    <Button
        android:id="@+id/UnaryOperator_Reci"
        android:text="1/x" />
    <Button
        android:id="@+id/Number_Click1"
        android:text="1" />
    <Button
        android:id="@+id/Number_Click2"
        android:text="2" />
    <Button
        android:id="@+id/Number_Click3"
        android:text="3" />
    <Button
        android:id="@+id/BinaryOperator_ClickSub"
        android:text="-" />
    <Button
        android:id="@+id/Equal_Click"
        android:layout_gravity="fill"
        android:layout_rowSpan="2"
        android:text="=" />
    <Button
        android:id="@+id/Number_Click0"
        android:layout_gravity="fill"
        android:layout_columnSpan="2"
        android:text="0" />
    <Button
        android:id="@+id/Number_ClickDot"
        android:text="." />
    <Button
        android:id="@+id/BinaryOperator_ClickAdd"
        android:text="+" />
</GridLayout>